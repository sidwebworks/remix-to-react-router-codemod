{
  "$schema": "https://codemod-utils.s3.us-west-1.amazonaws.com/configuration_schema.json",
  "name": "remix/2/react-router/upgrade",
  "description": "Migrates Remix to React Router v7",
  "version": "1.2.0",
  "engine": "jscodeshift",
  "private": false,
  "include": [
    "**/*.js",
    "**/*.cjs",
    "**/*.mjs",
    "**/*.ts",
    "**/*.jsx",
    "**/*.tsx",
    "**/package.json",
    "**/vite.config.ts",
    "**/tsconfig.json",
    "**/tsconfig.base.json"
  ],
  "applicability": {
    "from": [["@remix-run/react", ">=", "2.0.0"]]
  },
  "arguments": [],
  "meta": {
    "tags": ["migration", "remix", "react-router", "v7"],
    "git": "https://github.com/jrestall/react-router-codemods/tree/main/packages/remix-to-react-router"
  }
}
